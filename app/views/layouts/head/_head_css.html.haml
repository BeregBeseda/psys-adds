/[if lt IE 7]
  :css
    * {
        margin: 0;
    }
    html, body {
        height: 100%;
        overflow: auto;
    }
    .wrapper {
        position: relative;
        width: 100%;
        height: 100%;
        overflow: auto;
    }
    .box {
        position: fixed;
        left: 10px;
        top: 180px;
    }
    * html .box {
        position: absolute;
    }

/[if lt IE 9]
  :css
    #psycologist {
      position: static;
    }

